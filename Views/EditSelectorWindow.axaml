<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="WebLoadTester.Views.EditSelectorWindow"
        Title="Редактирование селектора"
        Width="480"
        Height="160"
        WindowStartupLocation="CenterOwner"
        CanResize="False">
	<Grid Margin="12" RowDefinitions="Auto,Auto,Auto" RowSpacing="10">
		<TextBlock Text="Селектор CSS" Margin="0,0,0,4"/>

		<TextBox x:Name="SelectorTextBox" Grid.Row="1" Watermark="css_selector_here" />

		<StackPanel Grid.Row="2" Orientation="Horizontal" Spacing="10" HorizontalAlignment="Right">
			<Button x:Name="OkButton" Content="ОК" MinWidth="80"/>
			<Button x:Name="CancelButton" Content="Отмена" MinWidth="80"/>
		</StackPanel>
	</Grid>
</Window>
