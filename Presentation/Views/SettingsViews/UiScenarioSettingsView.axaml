<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="WebLoadTester.Presentation.Views.SettingsViews.UiScenarioSettingsView">
    <StackPanel Spacing="6" Margin="12">
        <TextBlock Text="Target URL" />
        <TextBox Text="{Binding TargetUrl}" />
        <TextBlock Text="Total Runs" />
        <TextBox Text="{Binding TotalRuns}" />
        <TextBlock Text="Concurrency" />
        <TextBox Text="{Binding Concurrency}" />
        <CheckBox Content="Headless" IsChecked="{Binding Headless}" />
    </StackPanel>
</UserControl>
